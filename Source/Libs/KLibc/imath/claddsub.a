********** claddsub_a **********

*   addition and subtraction of longs

 psect claddsub_a,0,0,1,0,0

_ladd: ldd 4,s
 addd 2,x
 std _flacc+2,y
 ldd 2,s
 adcb 1,x
 adca ,x
 std _flacc,y
 lbra _lbexit

_lsub: ldd 4,s
 subd 2,x
 std _flacc+2,y
 ldd 2,s
 sbcb 1,x
 sbca ,x
 std _flacc,y
 lbra _lbexit
 endsect
